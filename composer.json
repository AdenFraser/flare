{
    "name": "laravelflare/flare",
    "description": "Laravel Flare",
    "keywords": [
        "Laravel", "Flare"
    ],
    "homepage": "https://github.com/laravelflare/flare",
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel Flare",
            "homepage": "https://github.com/laravelflare"
        },
        {
            "name": "Aden Fraser",
            "email": "aden@jacobbailey.com",
            "homepage": "https://github.com/AdenFraser"
        }
    ],
    "require": {
        "laravel/laravel": "5.1.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.1",
        "phpunit/phpunit": "~4.0",
        "orchestra/testbench": "~3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "LaravelFlare\\Flare\\": "src/Flare"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFlare\\Flare\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "laravelflare/cms": "The base component for adding CMS functionality to Flare.",
        "laravelflare/pages": "Provides a Pages module for the Flare CMS"
    }
}

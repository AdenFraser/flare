{
    "name": "laravelflare/flare",
    "description": "Laravel Flare",
    "keywords": [
        "Laravel", "Flare"
    ],
    "homepage": "https://github.com/laravelflare/flare",
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel Flare",
            "homepage": "https://github.com/laravelflare"
        },
        {
            "name": "Aden Fraser",
            "email": "aden@frasertechnical.com",
            "homepage": "https://github.com/AdenFraser"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.5", 
        "laravelflare/fields": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.5",
        "orchestra/testbench": "~3.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "LaravelFlare\\Flare\\": "src/Flare"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFlare\\Flare\\Tests\\": "tests/"
        }
    },
}
